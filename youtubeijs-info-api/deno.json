{
  "tasks": {
    "start":"deno run -R -W -E -N -S --cached-only src/index.ts",
    "dev": "deno run -R -W -E -N -S --watch src/index.ts",
    "install-deps": "deno install",
    "install-deps-force": "deno run install-deps -r",
    "update-deps": "deno outdated -u",
    "update-deps-force": "deno run update-deps --latest"
  },
  "imports": {
    "@fastify/type-provider-typebox": "npm:@fastify/type-provider-typebox@^6.1.0",
    "@sinclair/typebox": "npm:@sinclair/typebox@^0.34.47",
    "fastify": "npm:fastify@^5.7.1",
    "tracer": "npm:tracer@^1.3.0",
    "youtubei.js": "npm:youtubei.js@^16.0.1"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true
  }
}